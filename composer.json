{
  "name": "tourze/doctrine-paginator-bundle",
  "description": "Doctrine分页增强",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrinePaginatorBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrinePaginatorBundle\\Tests\\": "tests"
    }
  }
}